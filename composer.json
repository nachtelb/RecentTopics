        },
        {
            "name": "nickvergessen",
            "email": "",
            "homepage": "http://www.flying-bits.org/",
            "role": "Previous Developer of Recent Topics"
        },
        {
            "name": "Татьяна5",
            "homepage": "http://phpbbguru.net",
            "role": "Developer of Tabbed Categories"
        },
        {
            "name": "brunino",
            "email": "brunino@altervista.org",
            "homepage": "http://www.phpbbrevolution.6te.net",
            "role": "Developer of Union"
        }

    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Recent Topics with Tabbed Categories",
        "soft-require": {
             "phpbb/phpbb": ">=3.1.0,<3.2.*@dev"
        },
        "version-check": {
            "host": "paybas.github.io",
            "directory": "/RecentTopics",
            "filename": "version.json"
        }
    }
